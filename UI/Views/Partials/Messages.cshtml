

@if (ViewBag.ProcessState == DTO.General.Messages.success)
{
    <div class="alert alert-success" role="alert" dir="rtl">
        הפעולה הושלמה בהצלחה!
    </div>
}
else if (ViewBag.ProcessState == DTO.General.Messages.error)
{
    <div class="alert alert-warning" role="alert" dir="rtl">
        שגיאה!
    </div>
}
else if (ViewBag.ProcessState == DTO.General.Messages.dbError)
{
    <div class="alert alert-warning" role="alert" dir="rtl">
        שגיאה! יישוב זה קיים במערכת
    </div>
}


